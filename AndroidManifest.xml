<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.microgle.bore"
      android:versionCode="1"
      android:versionName="0.1">
    <application android:icon="@drawable/reader" android:label="@string/app_name">
        <activity android:name=".ReaderView" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
        <activity android:name=".ListTitleActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="com.ostrichmyself.txtReader.ListAllFileActivity" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		 
    </application>
    <uses-sdk android:minSdkVersion="3" />
</manifest> 